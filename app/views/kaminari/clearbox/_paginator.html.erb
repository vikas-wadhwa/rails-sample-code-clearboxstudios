<%= paginator.render do %>


    <ul class="pagination space-above space-below-4x" role="navigation" aria-label="Pagination">
        <%= prev_page_tag -%>

        <% each_page.select(&:inside_window?).each do |page| %>
              <%= page_tag page -%>
        <% end %>

        <%= next_page_tag -%>
    </ul>


<% end %>
